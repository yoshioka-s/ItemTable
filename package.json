{
  "name": "ItemTable",
  "version": "0.0.0",
  "description": "",
  "main": "scripts/app.jsx",
  "devDependencies": {
    "watch": "latest"
  },
  "scripts": {
    "start": "compass install bootstrap -r bootstrap-sass",
    "build": "npm run build-js & npm run build-sass",
    "build-js": "browserify scripts/app.jsx -o bundle.js",
    "build-sass": "compass compile .",
    "watch": "npm run watch-js & npm run watch-sass",
    "watch-js": "watch 'npm run build-js' .",
    "watch-sass": "compass watch"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yoshioka-s/ItemTable"
  },
