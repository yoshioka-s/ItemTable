{
  "manifest_version": 2,

  "name": "Shigotoshita",
  "description": "This extension tracks time you spent on your tasks.",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}
